<?xml version='1.0' encoding='utf-8'?>
<widget id="pl.artvawe.cordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Timelife</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        TamPham
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#165769" />
    <preference name="StatusBarStyle" value="blackopaque" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <preference name="CodePushDeploymentKey" value="uX1SHjFU6j8szzLRu6o5OqA2bv9rVJPK4Tn4z" />
    </platform>
    <platform name="ios">
        <preference name="CodePushDeploymentKey" value="uX1SHjFU6j8szzLRu6o5OqA2bv9rVJPK4Tn4z" />
    </platform>
    <plugin name="cordova-plugin-code-push" spec="~1.9.2-beta" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.1" />
    <engine name="ios" spec="~3.9.2" />
    <engine name="android" spec="~4.1.1" />
</widget>
