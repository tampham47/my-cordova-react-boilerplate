{
  "name": "cordova-react-boilerplate",
  "id": "pl.artvawe.cordova",
  "version": "1.1.2",
  "description": "Cordova app boilerplate",
  "scripts": {
    "postinstall": "gulp prepare-build",

    "release-ios": "code-push release-cordova tw-starter ios",
    "release-android": "code-push release-cordova tw-starter android",
