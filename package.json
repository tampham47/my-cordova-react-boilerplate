    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-preprocess": "^2.0.0",
    "gulp-shell": "^0.5.1",
    "gulp-util": "^3.0.7",
    "node-sass": "^3.4.2",
    "phonegap": "^5.4.1",
    "quick-local-ip": "^1.0.7",
    "react-hot-loader": "^1.3.0",
