  "main": "src/main.js",
  "homepage": "https://github.com/uhlryk/my-cordova-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/uhlryk/my-cordova-react-boilerplate.git"
  },
  "license": "MIT",
  "author": "Krzysztof Sztompka <sztompka.krzysztof@gmail.com>",
  "bugs": {
    "url": "https://github.com/uhlryk/my-cordova-react-boilerplate/issues"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "compass-mixins": "^0.12.7",
    "cordova": "^5.4.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
