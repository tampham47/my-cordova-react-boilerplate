    "gen-icon": "cordova-icon --xcode-old",
    "gen-splash": "cordova-splash --xcode-old"
  },
  "keywords": [
    "cordova",
    "gulp",
    "react",
    "redux",
    "jsx",
    "hot-loader",
    "es6",
    "es7",
    "babel",
    "webpack",
    "bootstrap3",
    "sass"
